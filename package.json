{
  "name": "untron-interface",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.16.0"
  },
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev:bridge": "cd apps/bridge && bun dev",
    "build:bridge": "cd apps/bridge && bun run build",
    "preview:bridge": "cd apps/bridge && bun run preview",
    "lint:bridge": "cd apps/bridge && bun run lint",
    "check:bridge": "cd apps/bridge && bun run check",
    "test:bridge": "cd apps/bridge && bun run test",
    "dev:v3": "cd apps/v3 && bun dev",
    "build:v3": "cd apps/v3 && bun run build",
    "preview:v3": "cd apps/v3 && bun run preview",
    "lint:v3": "cd apps/v3 && bun run lint",
    "check:v3": "cd apps/v3 && bun run check",
    "test:v3": "cd apps/v3 && bun run test",
    "dev:landing": "cd apps/landing && bun dev",
    "build:landing": "cd apps/landing && bun run build",
    "preview:landing": "cd apps/landing && bun run preview",
    "lint:landing": "cd apps/landing && bun run lint",
    "lint:landing:fix": "cd apps/landing && bun run lint:fix",
    "format:landing": "cd apps/landing && bun run format",
    "format:landing:fix": "cd apps/landing && bun run format:fix"
  }
}
